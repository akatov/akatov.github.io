{
  "name": "akatov.github.io",
  "version": "1.0.0",
  "description": "Dmitri's Blog",
  "scripts": {
    "build": "tsx node_modules/@11ty/eleventy/cmd.cjs --config=eleventy.config.ts --formats=11ty.ts,11ty.tsx",
    "start": "tsx node_modules/@11ty/eleventy/cmd.cjs --config=eleventy.config.ts --formats=11ty.ts,11ty.tsx --serve --incremental",
    "css:build": "postcss --output theme/static/style.css theme/templates/style.css",
    "css:dev": "postcss --watch --output theme/static/style.css theme/templates/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akatov/akatov.github.io.git"
  },
  "author": "Dmitri Akatov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/akatov/akatov.github.io/issues"
  },
  "homepage": "https://github.com/akatov/akatov.github.io#readme",
  "type": "module",
  "devDependencies": {
    "autoprefixer": "10.4.16",
    "postcss": "8.4.31",
    "postcss-cli": "10.1.0",
    "tailwindcss": "3.3.3",
    "tsx": "4.19.1"
  },
  "dependencies": {
    "@11ty/eleventy": "3.0.0-alpha.20",
    "jsx-async-runtime": "^0.5.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
